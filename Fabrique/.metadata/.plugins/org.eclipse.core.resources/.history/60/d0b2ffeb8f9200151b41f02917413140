
public class TestFabrique 
{
	public static void main(String[] args) 
	{
		// Airbus
		UsineJet u1 = new UsineJet("Toulouse");
		System.out.println(u1);
		
		Jet j1 = u1.fabriquerJet("A380", "Trent");
		System.out.println(j1);
		
		Jet j2 = u1.fabriquerJet("A320", "RollRoyce");
		System.out.println(j1);
		
		// Boeing
		UsineJet u2 = new UsineJet("Seatle");
		System.out.println(u2);
		
		Jet j3 = u2.fabriquerJet("B747", "RollRoyce");
		System.out.println(j3);
		
		Jet j4 = u2.fabriquerJet("B737", "Trent");
		System.out.println(j4);
	}
}
